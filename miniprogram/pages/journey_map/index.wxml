<view class="page full-screen">
  <map id="JourneyMap" style="width: 100%; height: 100vh;" bindtap="tapMap" bindmarkertap="markerTap" bindcallouttap="calloutTap" markers="{{markers}}" include-points>
    <cover-view class="self-position-btn">
      <cover-image src="../../assets/image/self-position.png" bindtap="tapSelfPosition"></cover-image>
    </cover-view>
    <cover-view class="show-all-btn">
      <cover-image src="../../assets/image/show-all.png" bindtap="tapShowAll"></cover-image>
    </cover-view>
  </map>
</view>
